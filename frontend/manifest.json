{
  "manifest_version": 3,
  "name": "AI家庭教師くん",
  "description": "AI家庭教師くんです。",
  "version": "1.0.0",
  "permissions": ["storage", "activeTab", "contextMenus", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content/index.js"]
    }
  ],
  "background": {
    "service_worker": "dist/background/index.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {},
  "icons": {
    "128": "kateikyoushi_woman_boy.png"
  }
}
